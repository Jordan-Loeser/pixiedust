{
  "name": "pixiedust-turborepo",
  "version": "1.0.0",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-cache --continue",
    "dev:new": "turbo run dev --no-cache --continue --parallel",
    "test": "turbo run test"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "pixiedust-applets/*"
  ],
  "nohoist": [
    "**/@types/**"
  ],
  "private": true,
  "devDependencies": {
    "turbo": "^1.10.14"
  }
}
